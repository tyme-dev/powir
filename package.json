{
  "name": "powir",
  "version": "1.0.5",
  "description": "Windows 10 based tool to monitor and analyze your system's power and battery usage",
  "main": "dist-electron/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlapBot/powir.git"
  },
  "author": {
    "name": "Ujjwal Gupta",
    "email": "ugupta41@gmail.com",
    "url": "https://slapbot.me/"
  },
  "keywords": [
    "electron",
    "battery",
    "windows-battery",
    "react",
    "battery-monitoring",
    "battery-analyzer"
  ],
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.0",
    "jssoup": "0.0.11",
    "lodash": "^4.17.15",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "vite": "^6.0.6"
  },
  "scripts": {
    "browser-start": "node public/browser.js&&set VITE_REACT_APP_LITE_MODE=1&&vite",
    "start": "set BROWSER=none&&set VITE_REACT_APP_LITE_MODE=0&&vite",
    "electron-start": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build": "vite build && electron-builder",
    "dev": "set VITE_REACT_APP_LITE_MODE=1&&vite"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
